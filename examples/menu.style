
.btns-group {
    margin-left: expand;
    margin-right: expand;
}

.btn {
    width: 200px;
    height: 40px;
    background-image: $btn.notselected;
    background-image-rule: fit;
}

.btn:focus {
    background-image: $btn.selected;
}

.btn-one-line {
    width: auto;
    height: 40px;
    background-image: $btn.notselected;
    background-image-rule: fit;
}

.btn-one-line:focus {
    background-image: $btn.selected;
}
