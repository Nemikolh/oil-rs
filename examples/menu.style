
.btns-group {
    margin-left: auto;
    margin-right: auto;
}

.btn-play {
    width: auto;
    height: 40px;
    background-image: $btn.selected;
    background-image-rule: fit;
}

.btn {
    width: 200px;
    height: 40px;
    background-image: $btn.notselected;
    background-image-rule: fit;
}

.btn-one-line {
    width: auto;
    height: 40px;
    background-image: $btn.notselected;
    background-image-rule: fit;
}
